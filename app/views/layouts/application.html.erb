<!DOCTYPE html>
<html>
  <head>
    <title>University</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <header class="navbar navbar-fixed-top navbar-inverse">
    <div class="container">
      <nav>
        <ul class="mainbar">
          <li><%= link_to 'Courses', courses_url %></li>
          <li><%= link_to 'Sections',   sections_url %></li>
          <li><%= link_to 'Students', students_url %></li>
          <li><%= link_to 'Enrollments', enrollments_url %></li>
        </ul>
      </nav>
    </div>
  </header>
    <%= yield %>
  </body>
</html>
